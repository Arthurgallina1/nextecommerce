{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src",
    "test": "jest",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test:watch": "yarn test --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test dev 3000 cy:open",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "generate": "yarn plop --plopfile ./generators/plopfile.js"
  },
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "@stripe/react-stripe-js": "^1.4.1",
    "@stripe/stripe-js": "^1.15.1",
    "@types/lodash.xor": "^4.5.6",
    "dotenv": "^10.0.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "graphql": "^15.5.0",
    "jest-styled-components": "^7.0.3",
    "joi": "^17.4.0",
    "lodash.xor": "^4.5.0",
    "next": "10.2.0",
    "next-auth": "^3.23.1",
    "next-pwa": "^5.0.5",
    "next-seo": "^4.26.0",
    "nextjs-progressbar": "^0.0.9",
    "polished": "^4.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.28.1",
    "styled-components": "^5.2.1",
    "styled-icons": "^10.28.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/react": "^6.1.17",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.1.2",
    "@testing-library/user-event": "^13.0.16",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.4",
    "@types/react-slick": "^0.23.4",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "apollo": "^2.32.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "cypress": "^7.7.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "msw": "^0.29.0",
    "node-fetch": "^2.6.1",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "start-server-and-test": "^1.12.6",
    "typescript": "^4.1.3"
  }
}
